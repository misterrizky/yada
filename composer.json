{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/livewire-starter-kit",
    "type": "project",
    "description": "The official Laravel starter kit for Livewire.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ashallendesign/short-url": "^8.4",
        "aws/aws-sdk-php": "^3.368",
        "barryvdh/laravel-dompdf": "^3.1",
        "bensampo/laravel-enum": "^6.12",
        "cjmellor/level-up": "^1.5",
        "cmgmyr/messenger": "^2.31",
        "creagia/laravel-sign-pad": "^2.1",
        "cviebrock/eloquent-sluggable": "^12.0",
        "cybercog/laravel-ban": "^4.10",
        "cybercog/laravel-love": "^10.1",
        "cyrildewit/eloquent-viewable": "^7.0",
        "genealabs/laravel-model-caching": "*",
        "genealabs/laravel-pivot-events": "*",
        "intervention/image": "^3.11",
        "intervention/image-laravel": "^1.5",
        "jeremykendall/php-domain-parser": "6.0",
        "kub-at/php-simple-html-dom-parser": "*",
        "lab404/laravel-impersonate": "^1.7",
        "laravel-lang/lang": "^15.26",
        "laravel/folio": "^1.1",
        "laravel/fortify": "^1.30",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "laraveldaily/laravel-charts": "^0.2.3",
        "laraveldaily/laravel-invoices": "^4.1",
        "league/flysystem-aws-s3-v3": "*",
        "league/oauth2-client": "*",
        "league/oauth2-google": "*",
        "livewire-filemanager/filemanager": "^1.0",
        "livewire/flux": "^2.9.0",
        "livewire/flux-pro": "^2.10",
        "livewire/volt": "^1.10",
        "maatwebsite/excel": "^3.1",
        "mavinoo/laravel-batch": "*",
        "mustache/mustache": "*",
        "opcodesio/log-viewer": "^3.21",
        "pelago/emogrifier": "*",
        "php-imap/php-imap": "^2.0",
        "phpoffice/phpspreadsheet": "^1.30",
        "plank/laravel-mediable": "*",
        "plank/laravel-metable": "*",
        "pusher/pusher-php-server": "*",
        "rappasoft/laravel-authentication-log": "^6.0",
        "soundasleep/html2text": "*",
        "spatie/icalendar-generator": "*",
        "spatie/image-optimizer": "^1.8",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-analytics": "^5.6",
        "spatie/laravel-backup": "^9.3",
        "spatie/laravel-db-snapshots": "^2.7",
        "spatie/laravel-failed-job-monitor": "^4.3",
        "spatie/laravel-feed": "^4.4",
        "spatie/laravel-health": "^1.34",
        "spatie/laravel-medialibrary": "^11.17",
        "spatie/laravel-model-status": "^1.18",
        "spatie/laravel-permission": "^6.24",
        "spatie/laravel-query-builder": "^6.3",
        "spatie/laravel-schedule-monitor": "^4.1",
        "spatie/laravel-schemaless-attributes": "^2.5",
        "spatie/laravel-server-monitor": "^1.10",
        "spatie/laravel-settings": "^3.6",
        "spatie/laravel-sitemap": "^7.3",
        "spatie/laravel-tags": "^4.10",
        "spatie/laravel-translatable": "*",
        "spatie/laravel-uptime-monitor": "^4.5",
        "spatie/period": "*",
        "symfony/finder": "*",
        "symfony/mailer": "*",
        "symfony/mime": "*",
        "symfony/process": "*",
        "wildside/userstamps": "^3.1",
        "willdurand/email-reply-parser": "*",
        "workos/workos-php": "^4.29",
        "xemlock/htmlpurifier-html5": "*",
        "zbateson/mail-mime-parser": "*"
    },
    "repositories": [
        {
            "type": "artifact",
            "url": "./packages"
        }
    ],
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/boost": "^1.8",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run build"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"npm run dev\" --names='server,queue,vite'"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan boost:update --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
